<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Social_Security_Minimum_Length</fullName>
    <active>false</active>
    <errorConditionFormula>LEN(LLC_BI__SS__c) &lt; 9</errorConditionFormula>
    <errorMessage>Invalid entry, Social Security Number field must contain at least nine characters</errorMessage>
</ValidationRule>
